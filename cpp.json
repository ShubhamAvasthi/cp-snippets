			"\t\tfor(int i = 0; i < d.size(); i++)",
			"\t\t{",
			"\t\t\t(d[i] *= i + 1) %= mod;",
			"\t\t}",
			"\t\treturn Poly_modular(d);",
			"\t}",
			"",
			"\tPoly_modular integrate() const",
			"\t{",
