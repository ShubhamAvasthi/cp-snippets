		"prefix": "totient",
		"body": [
			"long long phi(long long n)",
			"{",
			"\tlong long ans = n;",
			"\tfor(long long i = 2; i * i <= n; i++)",
			"\t\tif(n % i == 0)",
			"\t\t{",
			"\t\t\tans -= ans / i;",
			"\t\t\twhile(n % i == 0)",
			"\t\t\t\tn /= i;",
			"\t\t}",
			"\tif(n > 1)",
			"\t\tans -= ans / n;",
			"\treturn ans;",
